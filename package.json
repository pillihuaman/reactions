{
  "name": "reaction-sport",
  "version": "0.0.0",
  "private": true,
"scripts": {
  "ng": "ng",
  "start": "node dist/reaction-sport/server/main.js",
  "build": "ng build",
  "watch": "ng build --watch --configuration development",
  "test": "ng test",
  "lint": "ng lint",
  "clean": "rimraf dist node_modules package-lock.json",
  "build:prod": "ng build --configuration production",
  "build:dev": "ng build --configuration development",
  "build:ssr": "ng build && ng run reaction-sport:server:production",
  "serve:ssr": "node dist/reaction-sport/server/main.js",
  "prerender": "ng run reaction-sport:prerender:production",
  "dev:ssr": "ng run reaction-sport:serve-ssr",
  "build:full": "npm run build:ssr && npm run prerender"
}
,
  "dependencies": {
    "@angular/animations": "^19.0.0",
    "@angular/cdk": "^19.1.4",
    "@angular/common": "^19.0.0",
    "@angular/compiler": "^19.0.0",
    "@angular/core": "^19.0.0",
    "@angular/forms": "^19.0.0",
    "@angular/material": "^19.1.4",
    "@angular/material-moment-adapter": "^19.1.5",
    "@angular/platform-browser": "^19.0.0",
    "@angular/platform-browser-dynamic": "^19.0.0",
    "@angular/platform-server": "^19.0.0",
    "@angular/router": "^19.0.0",
    "@angular/ssr": "^19.2.0",
    "@auth0/angular-jwt": "^5.2.0",
    "bootstrap": "^5.3.3",
    "crypto-js": "^4.2.0",
    "eva-icons": "^1.1.3",
    "express": "^4.21.2",
    "moment": "^2.30.1",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^19.0.0",
    "@angular-devkit/build-angular": "^19.0.2",
    "@angular/cli": "^19.0.2",
    "@angular/compiler-cli": "^19.0.0",
    "@schematics/angular": "^19.0.2",
    "@types/crypto-js": "^4.2.2",
    "@types/express": "^5.0.0",
    "@types/jasmine": "~5.1.0",
    "@types/moment": "^2.11.29",
    "@types/node": "^22.13.5",
    "angular-eslint": "19.1.0",
    "eslint": "^9.20.0",
    "jasmine-core": "^5.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "rimraf": "^6.0.1",
    "sass": "^1.54.9",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.24.1"
  }
